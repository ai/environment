{
  "icon_theme": "Material Icon Theme",
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4"
    },
    "version": "2",
    "provider": {
      "name": "openai",
      "default_model": "gpt-4",
      "available_models": []
    }
  },
  "terminal": {
    "dock": "right"
  },
  "soft_wrap": "preferred_line_length",
  "preferred_line_length": 80,
  "wrap_guides": [80],
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "JetBrains Mono",
  "theme": {
    "mode": "system",
    "light": "Adwaita Pastel Light",
    "dark": "Adwaita Pastel Dark"
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  }
}
