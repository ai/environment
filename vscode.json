{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript][svelte][typescript][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    }
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "vscode.markdown-language-features"
  },
  "[postcss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[svelte]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "cSpell.language": "en,ru",
  "cSpell.userWords": [
    "andrey",
    "brotli",
    "brotlied",
    "browserslist",
    "catched",
    "CRDT",
    "esbuild",
    "globby",
    "gzipped",
    "logux",
    "nanodelay",
    "nanoevents",
    "nanospy",
    "nanostores",
    "oklab",
    "oklch",
    "pico",
    "pnpm",
    "Preact",
    "preadd",
    "sharec",
    "sitnik",
    "slowreader",
    "stylelint",
    "subprotocol",
    "unloadable",
    "vite",
    "Weblate"
  ],
  "css.validate": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "300",
  "editor.formatOnSave": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "selection",
  "editor.rulers": [
    80
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "eslint.migration.2_x": "off",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "svelte"
  ],
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "**/test/*.errors.ts": "rust",
    "*.bu": "yaml",
    "*.css": "postcss",
    "*.desktop": "ini",
    "*.ign": "json",
    "*.service": "ini",
    "*.timer": "ini",
    ".stylelintrc": "json",
    "errors.ts": "rust",
    "errors.tsx": "rust",
    ".tool-versions": "dotenv"
  },
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": true,
    "scminput": false
  },
  "less.validate": false,
  "nodejs-testing.extensions": [
    {
      "extensions": [
        "js"
      ],
      "parameters": []
    },
    {
      "extensions": [
        "ts"
      ],
      "parameters": [
        "--enable-source-maps",
        "--import",
        "tsx"
      ]
    }
  ],
  "prettier.arrowParens": "avoid",
  "prettier.quoteProps": "consistent",
  "prettier.requireConfig": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "projectManager.git.baseFolders": [
    "/home/ai/Dev/"
  ],
  "projectManager.git.maxDepthRecursion": 1,
  "scss.validate": false,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "stylelint.config": {
    "run": "onSave"
  },
  "svelte.enable-ts-plugin": true,
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontWeight": "300",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Adwaita Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "Adwaita Dark",
  "workbench.preferredLightColorTheme": "Adwaita Light",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": false,
  "eslint.experimental.useFlatConfig": true,
  "languageToolLinter.languageTool.language": "en",
  "languageToolLinter.languageTool.motherTongue": "ru-RU",
  "languageToolLinter.lintOnOpen": true,
  "languageToolLinter.languageTool.disabledRules": "COMMA_PARENTHESIS_WHITESPACE,UPPERCASE_SENTENCE_START,CONSECUTIVE_SPACES,WHITESPACE_RULE"
}
